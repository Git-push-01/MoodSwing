<div class="video-container">
<p id="notice"><%= notice %></p>

<p>
  <strong>Youtube embed:</strong>
  <%= @video.youtube_embed %>

  <iframe width="1000" height="500" src='https://www.youtube.com/embed/<%= @video.youtube_embed %>?rel=0&autoplay=<%= params[:autoplay] || 0 %>' frameborder='0' allowfullscreen></iframe>

<%= link_to 'Edit', edit_video_path(@video) %> |
<%= link_to 'Back', videos_path %>
<h3 class="header-container" ><%=link_to "All Songs", songs_path, class: "song-link" %></h3>
<h3 class="header-container" ><%= link_to "Back playlists", playlists_path, class: "song-link" %> </h3>
</div>
